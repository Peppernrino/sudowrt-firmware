config server
  option enabled '1'
  list addresses 'eth0.1=<%= extender_node_ips[0] %>/<%= open_subnet_ipv4_bitmask %>'
  list addresses 'eth0.2=<%= extender_node_ips[1] %>/<%= open_subnet_ipv4_bitmask %>'
  option hook_script '/opt/notdhcpserver/hook.sh'
  option ssl_cert '/etc/uhttpd.crt'
  option ssl_key '/etc/uhttpd.key'
  option timeout_length '180'
